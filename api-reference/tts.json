{
  "openapi": "3.1.0",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "https://tts.snr.audio/v1/rest",
      "description": "REST API Server"
    },
    {
      "url": "wss://tts.snr.audio/v1/rest/speech/ws",
      "description": "WebSocket Server"
    }
  ],
  "paths": {
    "/speech/turbo_stream": {
      "post": {
        "summary": "Streaming Audio Turbo",
        "operationId": "streaming_audio_turbo_v1_rest_speech_turbo_stream_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/receive_msg"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/speech/stream": {
      "post": {
        "summary": "Streaming Audio",
        "operationId": "streaming_audio_v1_rest_speech_stream_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/receive_msg"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        },
        "security": [
          {
            "KeyBearer": []
          }
        ]
      }
    },
    "/speech/list_speakers": {
      "get": {
        "summary": "Get Speaker",
        "operationId": "get_speaker_v1_rest_speech_list_speakers_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/speech": {
      "post": {
        "summary": "Main",
        "description": "Request processor.\n\n1. Generate the synthesized audio \n2. save in respective s3 bucket.\n\nArgs:\n    gen_tts (receive_msg) : request body in json format\n\nReturn: \n    dict / json str : with the url and details of the output.",
        "operationId": "main_v1_rest_speech_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/receive_msg"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        },
        "security": [
          {
            "KeyBearer": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "receive_msg": {
        "properties": {
          "input": {
            "type": "string",
            "title": "Input",
            "default": "hello world , Welcome to openMic"
          },
          "voice": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Voice"
          },
          "similarity": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Similarity",
            "default": 0.0
          },
          "expressiveness": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Expressiveness",
            "default": 0.0
          },
          "pitch": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Pitch",
            "default": 1.0
          },
          "speed": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Speed",
            "default": 1.0
          }
        },
        "type": "object",
        "title": "receive_msg"
      }
    },
    "securitySchemes": {
      "KeyBearer": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}
