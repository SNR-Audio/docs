            "description": "image URL"
          },
          "tags": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Tag"
            },
            "description": "tag"
          },
          "status": {
            "type": "string",
            "description": "Pet Sales Status",
            "enum": [
              "available",
              "pending",
              "sold"
            ]
          }
        }
